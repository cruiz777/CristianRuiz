<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CristianRuiz.Login">
    <ContentPage.Content>
        <StackLayout>
          
            <Image Source="https://sbsconsulting.com.ec/wp-content/uploads/2018/01/LOGO-UISRAEL-768x209.png " />
            <Label Text="Usuario:"></Label>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese Usuario"></Entry>
            <Label Text="Contraseña:"></Label>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese Contraseña" IsPassword="True"></Entry>
            <Button x:Name="Iniciar" Clicked="Iniciar_Clicked" Text="Iniciar sesion"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>